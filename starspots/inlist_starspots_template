&star_job

   history_columns_file = 'history_master.list'
   profile_columns_file = 'profile_master.list'

   create_pre_main_sequence_model = .true.

   change_net = .true.
   new_net_name = 'pp_extras.net'

   pgstar_flag = .false.

   ! save a model at the end of the run
   save_model_when_terminate = .true.
   save_model_filename = 'ms_model.mod'

   warn_run_star_extras = .false.

/ ! end of star_job namelist


&eos
/

&kap
   Zbase = 0.014
   kap_file_prefix = 'a09'
   kap_lowT_prefix = 'lowT_fa05_a09p'
   kap_CO_prefix = 'a09_co'
/

&controls
   initial_mass = 0.9

   ! Starspots
   do_starspots = .true.
   fspot = 0.34d0
   xspot = 0.85d0

   ! Stop at end of main sequence
   xa_central_lower_limit_species(1) = 'h1'
   xa_central_lower_limit(1) = 0.0000000001

   ! Atmosphere
   atm_option = 'table'
   atm_table = 'photosphere'

   ! Output
   log_directory = 'LOGS'
   history_interval = 1
   terminal_interval = 100
   photo_interval = 500
   write_profiles_flag = .false.

   ! Solver
   use_gold2_tolerances = .true.
   varcontrol_target = 1e-4

/ ! end of controls namelist


&pgstar
/

